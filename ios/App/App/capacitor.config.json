{
	"appId": "com.locationmonitor.app",
	"appName": "Location Monitor",
	"webDir": "out",
	"server": {
		"androidScheme": "https"
	},
	"plugins": {
		"Geolocation": {
			"enableBackgroundLocationUpdates": true
		},
		"BackgroundGeolocation": {
			"notificationTitle": "Location Tracking Active",
			"notificationText": "üìç Location Monitor is tracking your location",
			"enableHighAccuracy": true,
			"distanceFilter": 10,
			"interval": 10000,
			"fastestInterval": 5000,
			"backgroundInterval": 45000,
			"backgroundDistanceFilter": 15,
			"stale": false,
			"debug": false,
			"stopOnTerminate": false,
			"startOnBoot": false,
			"enableWakeLock": true,
			"enableForegroundService": true,
			"locationProvider": "ANDROID_GPS",
			"saveBatteryOnBackground": false,
			"activitiesInterval": 60000,
			"activityType": "AutomotiveNavigation",
			"notificationIconLarge": "@mipmap/ic_launcher",
			"notificationIconSmall": "@mipmap/ic_launcher",
			"priority": "PRIORITY_HIGH_ACCURACY",
			"maxWaitTime": 60000,
			"deferTime": 0,
			"autoSync": true,
			"batchSync": false,
			"disableElasticity": true,
			"elasticityMultiplier": 1,
			"disableStopDetection": true,
			"isDebugging": false
		},
		"CapacitorHttp": {
			"enabled": true
		}
	},
	"packageClassList": [
		"BackgroundGeolocation",
		"DevicePlugin",
		"GeolocationPlugin"
	]
}
